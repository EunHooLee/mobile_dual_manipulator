<launch>
    <param name="robot_description" command="$(find xacro)/xacro $(find dualarm_config)/urdf/dualarm_test2.xacro"/>
    <node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher"/>
    <node pkg="joint_state_publisher" type="joint_publisher" name="joint_state_publisher">
        <param name="use_gui" value="false"/>
    </node>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find dualarm_config)/urdf.rviz" required="True"/>
    

</launch>